	</head>

	<body>

		<!-- Start Align Area -->
			<div class="apply_form_area"> <div class="container">
				<div class="row justify-content-center">
					<div class="col-lg-8">
						<form method="post" action="confirm.php" class="apply_form">
							<div class="row">
								<div class="col-lg-12">
									<div class="apply_info_text text-center">
										<h3>登録確認</h3>
										<p>以下の情報で登録します</p>
									</div>
								</div>
								<div class="col-md-12">
									<div class="single_field">
										<p>ユーザー名 :
											{{dataArr.user_name}}</p>
									</div>
								</div>
								<div class="col-md-12">
									<div class="single_field">
										<p>メールアドレス :
											{{dataArr.email}}</p>
									</div>
								</div>
								<div class="col-md-12">
									<div class="single_field">
										<p>生年月日 :
											{{dataArr.year}}年{{dataArr.month}}月{{dataArr.day}}日</p>
									</div>
								</div>

								<div class="col-md-12">
									<div class="submit_btn">
										<button class="boxed-btn3" type="submit" name="complete">登録する</button>
									</div>
								</div>
								<div class="col-md-12">
									<div class="submit_btn">
										<button class="boxed-btn3" type="submit" name="back">戻る</button>
									</div>
								</div>
								<div>
									{% for key, value in dataArr %}
										{% if value is iterable %}
											{% for v in value %}
												<input type="hidden" name="{{key}}[]" value="{{v}}">
											{% endfor %}
										{% else %}
											<input type="hidden" name="{{key}}" value="{{value}}">
										{% endif %}
									{% endfor %}
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
		<!-- End Align Area -->

		<!-- footer start -->
		{% include "footer.twig" %}
		<!--/ footer end  -->

		<!-- JS here -->
		<script src="js/vendor/modernizr-3.5.0.min.js"></script>
