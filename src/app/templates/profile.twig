	<body>

		<!-- header-start -->
		{# isLoginでログイン済みかどうかを判定してマイページの表示を変える #}
		{% include "header.twig" with {'isLogin': isLogin} %}
		<!-- header-end -->

		<!-- bradcam_area  -->
			<div class="bradcam_area bradcam_bg_1"> <div class="container">
				<div class="row">
					<div class="col-xl-12">
						<div class="bradcam_text">
							<h3>{{userInfo.user_name}}</h3>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!--/ bradcam_area  -->

		<!-- Start Align Area -->
		<div class="whole-wrap">
			<div class="container box_1170">

				<div class="section-top-border">
					<div
						class="row">
						{# 左側 #}
						<div class="col-lg-8 col-md-8">
							<h3 class="mb-30">{{userInfo.user_name}}さんのプロフィール</h3>
							<div class="bg-light border p-3">
								<p class="text-center">ユーザー名</p>
								<p class="text-center">{{userInfo.user_name}}</p>
								<p class="text-center">メールアドレス</p>
								<p class="text-center">{{userInfo.email}}</p>
								<p class="text-center">プロフィール画像</p>
								<br>
								<img class="d-block mx-auto mb-3" src="./img/profile/{{userInfo.image}}" alt="プロフィール画像">
								<p class="text-center">生年月日</p>
								<p class="text-center">{{userInfo.year}}年{{userInfo.month}}月{{userInfo.day}}日</p>
							</div>


							<div class="user-wrapper clearFix">
								<div class="user-prof">
									<div class="user-icon">
										<a href="/users/nakaizawa">
											<img src="./img/profile/{{userInfo.image}}" class="prof-img-m" width="84" height="84" alt="ユーザー">
										</a>
									</div>

									<div class="user-social">
										<div class="user-follow-btn">
											<div class="follow-area" data-account="nakaizawa">
												<a href="#" class="follow-btn">
													<i class="fa fa-plus"></i>
													フォローする</a>
											</div>
										</div>

										<script id="source-follow-btn" type="text/jarty">
											{*
											<![CDATA[
											*
} < a href = "#" class = "follow-btn" > <i class="fa fa-plus"> < /i> フォロー</a > { * ]]>
											*}
										*}
										</script>

										<script id="source-unfollow-btn" type="text/jarty">
											{*
											<![CDATA[
											*
} < a href = "#" class = "unfollow-btn" > </a> { * ]]>
											*}
										*}
										</script>

										<script id="source-error-follow-btn" type="text/jarty">
											{*
